CONFIG
TRACKTYPE: 1;
END;

INPUT
variable_group x0, x1, x2;
function f0, f1, f2;
f0 = x0^3*x1*x2^3 + 2*x0*x1^3*x2^3 + 3*x0^3*x1^2*x2 - 4*x0^2*x1^2*x2 - 3*x0^2*x2^3 + x0^3*x1 - x0^3*x2 - 3*x0*x2^3 - 3*x0*x2^2 + 3*x0^2;
f1 = 3*x0^2*x1^3 - 3*x0*x1^3*x2 - 2*x0*x1^2*x2^2 - 2*x0*x1^3 + 2*x0*x2^3 - 4*x1^3 + 2*x0^2*x2 - 4*x1*x2;
f2 = 2*x0^3*x1^3*x2^2 + x0^2*x1^3*x2^3 - 3*x0^3*x1^3*x2 + 2*x0^2*x1^3*x2^2 - 3*x0^3*x1*x2^3 + 2*x0^3*x2^3 - x0^2*x2^3 - 4*x0^3 + 3*x1*x2;
END;
